
<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Access</th>
    </tr>
  </thead>
  <tbody>
    <% @users = User.where.not(id: current_user.id) %>
    <% @users.each do |user| %>
      <tr>
        <td>Column content</td>
        <td><%= user.email %></td>
        <td>
          <%= link_to 'Add', collaborations_path(user_id: user.id, wiki_id: wiki.id), method: :post, class: 'btn btn-success' %>
          <%= link_to 'Remove', collaboration_path(user_id: user.id, wiki_id: wiki.id), method: :delete, class: 'btn btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
