<div class="row">
  <h1 class="text-center">Wikis</h1>
  <div class="col-md-12 text-center">
    <%= link_to 'Create Wiki', new_wiki_path, :class => 'btn btn-primary btn-lg' %>
  </div>
</div>
<div class="row">
  <% @wikis.each do |wiki| %>
    <h3><%= link_to wiki.title, wiki %></h3>
  <% end %>
</div>
