<div class="container">
  <div class="row">
    <h1 class="text-center">My Wikis</h1>
  </div>
  <div class="row justify-content-center">
    <div class="col-4">
      <% @wikis.each do |wiki| %>
        <h3><%= link_to wiki.title, wiki %></h3>
      <% end %>
    </div>
    <div class="col-4">
      <%= link_to 'Create Wiki', new_wiki_path, :class => 'btn btn-primary btn-lg' %>
    </div>
  </div>
</div>
