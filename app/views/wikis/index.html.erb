<div class="row">
  <h1 class="text-center">Wikis</h1>
  <div class="col-md-12 text-center">
    <%= link_to 'Create Wiki', new_wiki_path, :class => 'btn btn-primary btn-lg' %>
  </div>
</div>
<div class="row">
  <p class="lead text-center">List of wikis</p>
  <%= @wikis.each do |wiki| %>
    <h4><%= wiki.title %></h4>
    <p><%= wiki.body %></p>
  <% end %>
</div>
